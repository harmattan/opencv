#! /bin/sh

set -e

update-alternatives --install /usr/lib/liblapack.so liblapack.so /usr/lib/lapack/liblapack.so 20 \
     --slave /usr/lib/liblapack.a liblapack.a /usr/lib/lapack/liblapack.a

# Remove previous (deprecated) alternatives
# See bug #576801
update-alternatives --remove liblapackgf-3.a /usr/lib/liblapack.a
update-alternatives --remove liblapackgf-3.so /usr/lib/liblapack.so

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


